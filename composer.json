{
    "name": "tienvx/ux-collection-js",
    "type": "symfony-bundle",
    "description": "Symfony UX integration for collection form type",
    "keywords": [
        "symfony-ux",
        "ux-collection-js"
    ],
    "homepage": "https://github.com/tienvx/ux-collection-js",
    "license": "MIT",
    "authors": [
        {
            "name": "Tien",
            "email": "tien.xuan.vo@gmail.com"
        },
        {
            "name": "Community contributions",
            "homepage": "https://github.com/tienvx/ux-collection-js/contributors"
        }
    ],
    "autoload": {
        "psr-4": {
            "Tienvx\\UX\\CollectionJs\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tienvx\\UX\\CollectionJs\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.4|^8.0",
        "symfony/config": "^4.4.17|^5.0",
        "symfony/dependency-injection": "^4.4.17|^5.0",
        "symfony/form": "^4.4.17|^5.0",
        "symfony/http-kernel": "^4.4.17|^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/framework-bundle": "^4.4.17|^5.0",
        "symfony/twig-bundle": "^4.4.17|^5.0",
        "symfony/var-dumper": "^4.4.17|^5.0"
    },
    "conflict": {
        "symfony/flex": "<1.13"
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0-dev"
        },
        "thanks": {
            "name": "ruano-a/symfonyCollectionJs",
            "url": "https://github.com/ruano-a/symfonyCollectionJs"
        }
    }
}
